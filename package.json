{
  "name": "preact-feather",
  "version": "4.0.0",
  "description": "Preact components for Feather icons",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "umd:main": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "exit 0",
    "build": "node ./bin/build && yarn build:es && yarn build:main",
    "build:es": "microbundle -f es --no-compress",
    "build:main": "microbundle -f cjs,umd",
    "prettier": "prettier {src,bin}/**/*.{js,ts,tsx} --write"
  },
  "repository": "ForsakenHarmony/preact-feather.git",
  "keywords": [
    "react",
    "preact",
    "icons",
    "svg",
    "inline",
    "feather",
    "design"
  ],
  "author": "Leah <me@hrmny.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForsakenHarmony/preact-feather/issues"
  },
  "homepage": "https://github.com/ForsakenHarmony/preact-feather#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "camelcase": "^5.3.1",
    "cheerio": "^1.0.0-rc.3",
    "glob": "^7.1.4",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "microbundle": "^0.12.0-next.6",
    "ncp": "^2.0.0",
    "preact": "^10.0.0",
    "prettier": "^1.18.2",
    "uppercamelcase": "^3.0.0"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,bin}/**/*.{js,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
